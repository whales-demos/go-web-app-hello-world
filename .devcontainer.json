{
    "image":"philippecharriere494/kube-demo-workspace:0.0.0",
    "mounts": [
        "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go",
                "pomdtr.excalidraw-editor",
                "Tobermory.es6-string-html"
            ]
        }
    },
    "remoteEnv": {
        "MESSAGE": "Hello World",
        "KUBECONFIG": "/workspaces/go-web-app-hello-world/.k8s/civo-red-panda-kubeconfig.yaml"
    }
}
